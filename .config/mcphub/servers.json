{
  "mcpServers": {
    "fetch": {
      "args": [
        "run",
        "-i",
        "--rm",
        "mcp/fetch"
      ],
      "command": "docker"
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "~/code"
      ],
      "command": "npx"
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "disabled": false
    }
  }
}
